{
  "name": "moltspay",
  "version": "0.4.3",
  "description": "Blockchain payment infrastructure for AI Agents on Moltbook",
  "keywords": [
    "moltbook",
    "blockchain",
    "payment",
    "usdc",
    "ai-agent",
    "web3",
    "eip-2612",
    "base",
    "ethereum",
    "moltspay"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./wallet": "./dist/wallet/index.js",
    "./permit": "./dist/permit/index.js",
    "./chains": "./dist/chains/index.js",
    "./x402": "./dist/x402/index.js",
    "./cdp": "./dist/cdp/index.js"
  },
  "bin": {
    "moltspay": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "peerDependencies": {
    "ethers": "^6.0.0",
    "@coinbase/cdp-sdk": "^1.0.0",
    "@x402/fetch": "^2.3.0",
    "@x402/evm": "^2.3.1"
  },
  "peerDependenciesMeta": {
    "@coinbase/cdp-sdk": { "optional": true },
    "@x402/fetch": { "optional": true },
    "@x402/evm": { "optional": true }
  },
  "devDependencies": {
    "@coinbase/cdp-sdk": "^1.0.0",
    "@types/node": "^20.11.0",
    "@x402/core": "^2.3.1",
    "@x402/evm": "^2.3.1",
    "@x402/fetch": "^2.3.0",
    "eslint": "^8.56.0",
    "ethers": "^6.11.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Yaqing2023/moltspay.git"
  },
  "homepage": "https://github.com/Yaqing2023/moltspay#readme",
  "bugs": {
    "url": "https://github.com/Yaqing2023/moltspay/issues"
  },
  "author": "Yaqing2023",
  "license": "MIT"
}
